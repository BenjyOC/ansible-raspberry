[user]
  name = Benjamin Rabiller
  email = xxx.xxx@gmail.com
[alias]
    st = status
    graph = log --graph --pretty --decorate --oneline
    grall = log --graph --pretty --decorate --oneline --all
[color]
    ui = true
[color "branch"]
  current = yellow reverse
  local = yellow
  remote = green
[color "diff"]
  meta = yellow bold
  frag = magenta bold
  old = red bold
  new = green bold
[color "status"]
  added = yellow
  changed = green
  untracked = cyan
[log]
    decorate = full
[help]
    autocorrect = 1

[push]
  default = matching
